<app>
	<entities>
		<entity name="user">
			<attributes>
				<attribute name="username" type="string" nullable="false"/>
				<attribute name="password" type="string" nullable="false"/>
				<attribute name="firstName" type="string" nullable="false"/>
				<attribute name="lastName" type="string" nullable="false"/>
				<attribute name="email" type="string" nullable="false"/>
			</attributes>
			<relations>
				<relation entityName="transaction" relationType="oneToMany" owningSide="false"/>
				<relation entityName="bankAccount" relationType="oneToMany" owningSide="true"/>
				<relation entityName="permission" relationType="manyToMany" owningSide="true"/>
			</relations>
		</entity>
		<entity name="permission">
			<attributes>
				<attribute name="name" type="string" nullable="false"/>
			</attributes>
			<relations>
				<relation entityName="user" relationType="manyToMany" owningSide="false"/>
			</relations>
		</entity>
		<entity name="bankAccount">
			<attributes>
				<attribute name="accountNumber" type="string" nullable="false"/>
			</attributes>
			<relations>
				<relation entityName="user" relationType="manyToOne" owningSide="false"></relation>
			</relations>
		</entity>
		<entity name="transaction">
			<attributes>
				<attribute name="date" type="date" nullable="false"/>
				<attribute name="accountNumber" type="string" nullable="false"/>
				<attribute name="ammount" type="string" nullable="false"/>
				<attribute name="sender" type="string" nullable="false"/>
				<attribute name="recipient" type="string" nullable="false"/>
				<attribute name="description" type="string" nullable="true"/>
				<attribute name="currency" type="string" nullable="false"/>
			</attributes>
			<relations>
				<relation entityName="user" relationType="manyToOne" owningSide="true"/>
				<relation entityName="stock" relationType="manyToOne" owningSide="true"/>
				<relation entityName="QRCode" relationType="oneToOne" owningSide="true"/>
				<relation entityName="paymentCode" relationType="oneToOne" owningSide="true"/>
			</relations>
		</entity>
		<entity name="paymentCode">
			<attributes>
				<attribute name="form" type="string" nullable="false"/>
				<attribute name="basis" type="string" nullable="false"/>
				<attribute name="description" type="string" nullable="false"/>
			</attributes>
			<relations>
				<relation entityName="transaction" relationType="oneToOne" owningSide="false"></relation>
			</relations>
		</entity>
		<entity name="stock">
			<attributes>
				<attribute name="name" type="string" nullable="false"/>
			</attributes>
			<relations>
				<relation entityName="transaction" relationType="oneToMany" owningSide="false"/>				
				<relation entityName="stockValue" relationType="oneToMany" owningSide="true"/>
			</relations>
		</entity>
		<entity name="stockValue">
			<attributes>
				<attribute name="value" type="string" nullable="false"/>
				<attribute name="date" type="date" nullable="false"/>
				<attribute name="currency" type="string" nullable="false"/>
				<attribute name="volume" type="string" nullable="false"/>
				<attribute name="traffic" type="string" nullable="false"/>
				<attribute name="volumeWeightAveragePrice" type="string" nullable="false"/>
				<attribute name="totalSupply" type="string" nullable="false"/>
				<attribute name="totalDemand" type="string" nullable="false"/>
			</attributes>
			<relations>
				<relation entityName="stock" relationType="manyToOne" owningSide="false"/>
			</relations>
		</entity>
		<entity name="QRCode">
			<attributes>
				<attribute name="code" type="string"/>
				<attribute name="date" type="date"/>
			</attributes>
			<relations>
				<relation entityName="transaction" relationType="oneToOne" owningSide="false"/>
			</relations>
		</entity>
	</entities>
	<enums>
		<enum name="permissionType">
			<values>
				<value>admin</value>
				<value>developer</value>
				<value>regular_user</value>
			</values>
		</enum>
	</enums>
</app>
